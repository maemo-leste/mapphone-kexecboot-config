LABEL=Maemo Leste
PRIORITY=8
DTB=/boot/omap4-razr-xt910.dtb
KERNEL=/boot/zImage
CMDLINE=console=tty0 fbcon=rotate:1 debug earlycon ro rootwait rootfstype=ext4 root=/dev/mmcblk0p2 softlevel=charge-mode
TIMEOUT=5

LABEL=Maemo Leste Emergency Shell
PRIORITY=7
DTB=/boot/omap4-razr-xt910.dtb
KERNEL=/boot/zImage
CMDLINE=console=tty0 console=ttyS2,115200 debug earlycon ro rootwait rootfstype=ext4 root=/dev/mmcblk0p2 softlevel=shell
TIMEOUT=5
